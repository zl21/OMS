{
  "name": "@burgeon/business-components",
  "version": "2.0.11",
  "description": "burgeon company's business-components lib for private npm",
  "main": "burgeon.publish/businessComponents.min.js",
  "files": [
    "burgeon.publish"
  ],
  "scripts": {
    "dev": "webpack serve --progress --color --progress --config webpack.publish.js --mode development ",
    "build": "cross-env BUILD_ENV=publish webpack --color --progress --config webpack.publish.js",
    "report": "npm run build --report",
    "push": "npm run build && yalc push",
    "watch": "nodemon --ignore burgeon.publish/ --ignore node_modules/ -C -e js,vue,less,scss --debug -x 'npm run push'"
  },
  "scriptsComments": {
    "push": "本地调试组件库包体并执行自动化",
    "watch": "自动监听本地化类库代码变更后同步调试信息"
  },
  "repository": {
    "type": "git",
    "url": "git@gitee.com:product_centerfront_end/burgeon-private-npm.git"
  },
  "author": "flybird <duan.ry@burgeon.cn>",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
